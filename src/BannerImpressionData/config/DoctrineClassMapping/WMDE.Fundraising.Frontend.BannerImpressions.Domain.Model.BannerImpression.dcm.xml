<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd"
>
    <entity name="WMDE\Fundraising\Frontend\BannerImpressionData\Domain\BannerImpression" table="banner_impressions">
        <indexes>
            <index name="idx_banner_name" columns="banner_name"/>
        </indexes>

        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>

        <field name="bannerName" type="string" column="banner_name" length="255" nullable="true"/>
        <field name="impressionCount" type="integer" column="impression_count" nullable="false"/>
        <field name="intervalStart" type="datetime" column="interval_start" nullable="true"/>
    </entity>
</doctrine-mapping>
