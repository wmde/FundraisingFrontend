{% extends 'Base_Layout.html.twig' %}

{% block main %}
	<div id="app">
		<noscript>
			<h2>{$ site_metadata.no_javascript_header $}</h2>
			{$ web_content( 'pages/no_javascript' ) $}
		</noscript>
	</div>
	<div id="appdata"
		 class="is-hidden"
		 data-application-vars="{$ faq_content|e('html_attr') $}"
		 data-application-messages="{$ translations()|e('html_attr') $}"
		 data-assets-path="{$ asset( '', 'skin') $}">
	</div>
{% endblock %}

{% block scripts %}
	{$ parent() $}
	<script src="{$ asset( 'js/frequent_questions.js', 'skin' ) $}"></script>
{% endblock %}
